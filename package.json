{
  "name": "chatterbot",
  "version": "0.1.0",
  "description": "Advanced Discord AI bot powered by Google Gemini.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:health": "tsx src/dev/health-dev.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "check:all": "npm run typecheck && npm run lint && npm test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:detect": "jest --detectOpenHandles",
    "docker:build": "docker build -t chatterbot .",
    "docker:run": "docker run --rm -it -e DISCORD_TOKEN -e DISCORD_CLIENT_ID -e GEMINI_API_KEY chatterbot",
    "docker:stop": "echo 'No docker-compose.yml in repo; stop container via docker stop <id>'",
    "docker:logs": "echo 'Use your container runtime logs viewer (no compose file in repo)'",
    "deploy:prod": "npm run build && npm run docker:build && npm run docker:run",
    "deploy:analytics": "echo 'No docker-compose.yml in repo; use ENABLE_ANALYTICS_DASHBOARD to start built-in dashboard'",
    "db:migrate": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "docs": "typedoc"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.25.0",
    "@google/generative-ai": "^0.7.1",
    "@mistralai/mistralai": "^1.1.0",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@modelcontextprotocol/server-brave-search": "^0.6.2",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.7.1",
    "@prisma/client": "^6.13.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "groq-sdk": "^0.6.0",
    "lodash": "^4.17.21",
    "openai": "^4.53.0",
    "lodash": "^4.17.21",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.4",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.55.0",
    "fast-check": "^4.2.0",
    "jest": "^29.7.0",
    "prisma": "^6.13.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.0",
    "typedoc": "^0.26.10",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
